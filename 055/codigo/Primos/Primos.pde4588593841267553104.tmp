
int colunas_qtd = 10;
int linhas_qtd = 48;

void setup() {
  size(500, 500);
}

void draw() {
  float celula_largura = width/colunas_qtd;
  float celula_altura = height/linhas_qtd;
  for (int i = 0; i < linhas_qtd; i++) {
    for (int j = 0; j < colunas_qtd; j++) {
      float x = j * celula_largura;
      float y = i * celula_altura;
      rect(x, y, celula_largura, celula_altura);
    }
  }
}

boolean checarPrimo(int n) {
  boolean checar = true;
  if (n <=2 ) {
    return checar;
  }
  for (int divisor =2; divisor < n; divisor++) {
    if (n % divisor == 0) {
      checar = false;
      return checar;
    }
  }
  return checar;
}
